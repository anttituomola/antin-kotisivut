{
  "buildCommand": "cd apps/website && npm install && npm run build",
  "outputDirectory": "apps/website/dist",
  "framework": "astro",
  "installCommand": "echo 'Skipping root install'",
  "rewrites": [
    { "source": "/(.*)", "destination": "/apps/website/dist/$1" }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
} 